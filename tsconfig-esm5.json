{
    "extends": "./tsconfig-esm2015.json",
    "compilerOptions": {
        "target": "es5",
        "rootDir": "./tmp",
        "outDir": "./tmp/esm5"
    },
    "angularCompilerOptions": {
        "annotateForClosureCompiler": true,
        "strictMetadataEmit": true,
        "skipTemplateCodegen": true,
        "flatModuleOutFile": "ng-hello-world.js",
        "flatModuleId": "ng-hello-world"
    }
}